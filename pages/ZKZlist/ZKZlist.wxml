<!--BMlist.wxml-->
<view class="wapper">
<view class="list clearfix" wx:for="{{dataList}}" data-zkz="{{item.licence}}" bindtap="toDetail">
<image class="photo" src="{{item.photoPath}}"></image>
<view class="info-wapper">
<view class="tr">
<label class="label">姓名：</label>
<view class="name">{{item.examineeName}}</view>
</view>
<view class="tr">
<label class="label">性别：</label>
<view class="name">{{item.sex}}</view>
</view>
<view class="tr">
<label class="label">准考证号：</label>
<view class="name">{{item.licence}}</view>
</view>
<view class="tr">
<label class="label">出生日期：</label>
<view class="name">{{item.birthday}}</view>
</view>
</view>
</view>


</view>
